f = open('purchase_log.txt')
file = f.read().split("\n")[:-1]
purchases = {}

for line in file:
    key = line.strip("}").strip("{").replace('"user_id":','').replace('"category":','').split(',')[0]
    value = line.strip("}").strip("{").replace('"user_id":','').replace('"category":','').split(',')[1]
    purchases[key] = value    

print(purchases) 
